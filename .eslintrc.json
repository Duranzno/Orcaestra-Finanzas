{
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2018
  },
  "env": {
    "browser": false,
    "es6": false,
    "node": true
  },
  "rules":{
    "func-style": "warn",
    "no-undef":"off",
    "no-console": "off",
    "no-unused-expressions": "off",
    "no-unused-vars":"warn"
  }
}