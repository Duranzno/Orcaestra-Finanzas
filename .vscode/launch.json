{
    "version": "0.1.0",
    "configurations": [
        {
            "name": "Localhost",
            "runtimeExecutable": "/usr/bin/flashpeak-slimjet",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:1234/",
            "webRoot": "${workspaceFolder}/src/public/js"
            // "sourceMapPathOverrides":{"${workspaceFolder}/js/datatables"}
        },
        {
            "type": "node",
            "request": "attach",
            "name": "Node: Nodemon",
            "processId": "${command:PickProcess}",
            "restart": true,
            "protocol": "inspector",
        },
    ]
}